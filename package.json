{
  "name": "septemberui",
  "author": {
    "name": "Mithicher Baro",
    "email": "mithnehu@gmail.com"
  },
  "keywords": [
    "tailwindcss",
    "vue",
    "septemberui",
    "vue components",
    "tailwind components"
  ],
  "version": "0.0.9",
  "description": "Component library made with vuejs and tailwindcss",
  "main": "dist/septemberui.ssr.js",
  "module": "dist/septemberui.esm.js",
  "unpkg": "dist/septemberui.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue",
    "!src/lib-dev.vue"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "dependencies": {
    "moment": "^2.24.0",
    "pikaday": "^1.8.0",
    "prismjs": "^1.17.1",
    "quill": "^1.3.7",
    "mailtoui": "^1.0.3",
    "autosize": "^4.0.2",
    "vue-prism-editor": "^0.3.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-for-of": "^7.7.4",
    "@rollup/plugin-alias": "^2.2.0",
    "@rollup/plugin-buble": "^0.20.0",
    "@rollup/plugin-replace": "^2.2.1",
    "cross-env": "^6.0.3",
    "minimist": "^1.2.0",
    "node-sass": "^4.13.0",
    "rollup": "^1.26.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-terser": "^5.1.2",
    "rollup-plugin-vue": "6.0.0",
    "sass-loader": "^8.0.0",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.6.10"
  },
  "engines": {
    "node": ">=8"
  },
  "homepage": "http://september-ui.herokuapp.com/"
}
